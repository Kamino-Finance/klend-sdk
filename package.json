{
  "name": "@kamino-finance/klend-sdk",
  "version": "5.10.22",
  "description": "Typescript SDK for interacting with the Kamino Lending (klend) protocol",
  "repository": {
    "type": "git",
    "url": "git+github.com:Kamino-Finance/klend-sdk.git"
  },
  "author": "Kamino Finance",
  "homepage": "https://kamino.finance",
  "bugs": {
    "url": "https://github.com/Kamino-Finance/klend-sdk/issues"
  },
  "keywords": [
    "solana",
    "anchor",
    "klend",
    "kamino"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/**/*.json",
    "src/**/*.ts"
  ],
  "license": "MIT",
  "scripts": {
    "docs": "typedoc",
    "build": "rm -rf dist/; tsc",
    "build:watch": "yarn tsc --watch",
    "watch": "tsc --watch",
    "coverage": "jest --coverage",
    "clean": "rm -rf dist/",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "yarn clean && yarn clean:deps",
    "lint": "yarn prettier --check .; yarn eslint .",
    "lint:fix": "yarn prettier --write .; yarn eslint . --fix",
    "codegen": "yarn anchor-client-gen src/idl.json ./src/idl_codegen/ --program-id KLend2g3cP87fffoy8q1mQqGKjrxjC8boSyAYavgmjD",
    "codegen-jupiter-perps": "yarn anchor-client-gen src/jupiter_perps_idl.json ./src/idl_codegen_jupiter_perps/ --program-id PERPHjGBqRHArX4DySjwM6UJHiR3sWAatqfdBS2qQJu",
    "codegen-kvault": "yarn anchor-client-gen src/idl_kamino_vault.json ./src/idl_codegen_kamino_vault/ --program-id kvauTFR8qm1dhniz6pYuBZkuene3Hfrs1VQhVRgCNrr",
    "cli": "tsx --no-deprecation src/client.ts",
    "kamino-manager": "tsx src/client_kamino_manager.ts",
    "dev": "concurrently \"yarn build:watch\" \"nodemon --watch dist -e js,ts --exec 'yarn dev:push-and-update'\"",
    "dev:push-and-update": "yalc publish && cd examples && yalc update"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.28.0",
    "@coral-xyz/borsh": "^0.28.0",
    "@kamino-finance/farms-sdk": "2.3.4",
    "@kamino-finance/kliquidity-sdk": "^7.0.1",
    "@kamino-finance/scope-sdk": "^8.0.3",
    "@solana/buffer-layout": "^4.0.0",
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.95.8",
    "@switchboard-xyz/sbv2-lite": "^0.1.0",
    "axios": "^1.6.8",
    "base58-js": "^2.0.0",
    "bn.js": "^5.2.1",
    "bs58": "^6.0.0",
    "buffer": "^6.0.3",
    "commander": "^9.3.0",
    "decimal.js": "^10.4.3",
    "exponential-backoff": "^3.1.1",
    "uuid": "^10.0.0",
    "zstddec": "^0.1.0"
  },
  "devDependencies": {
    "@orca-so/sdk": "^1.2.25",
    "@orca-so/whirlpool-sdk": "^0.4.2",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.54",
    "@types/bn.js": "^5.1.1",
    "@types/bs58": "^4.0.1",
    "@types/chai": "^4.3.5",
    "@types/jest": "^27.0.3",
    "@types/mocha": "^10.0.10",
    "@types/node-fetch": "^2.6.7",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "anchor-client-gen": "^0.28.1",
    "chai": "^4.3.7",
    "chai-bignumber": "^3.1.0",
    "concurrently": "^9.1.0",
    "eslint": "^8.4.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-react": "^7.27.1",
    "jest": "^27.4.3",
    "jest-fetch-mock": "^3.0.3",
    "mocha": "^10.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^28.0.7",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typedoc": "^0.22.10",
    "typescript": "^5.7.2",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
